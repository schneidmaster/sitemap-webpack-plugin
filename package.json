{
  "name": "sitemap-webpack-plugin",
  "version": "0.8.1",
  "description": "Webpack plugin to generate a sitemap.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build",
    "test": "jest",
    "test:webpack4": "yarn add webpack@^4 --dev && jest",
    "test:webpack5": "yarn add webpack@^5 --dev && jest",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "format": "prettier --write '**/*.js'",
    "lint": "eslint src/*.js 'test/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schneidmaster/sitemap-webpack-plugin.git"
  },
  "keywords": [
    "webpack",
    "plugin",
    "sitemap",
    "static"
  ],
  "author": "Zach Schneider <zach@schneid.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schneidmaster/sitemap-webpack-plugin/issues"
  },
  "files": [
    "lib/",
    "src/",
    "LICENSE.txt",
    "package.json",
    "README.md"
  ],
  "homepage": "https://github.com/schneidmaster/sitemap-webpack-plugin#readme",
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "async": "^2.1.4",
    "babel-eslint": "^10.0.3",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "glob": "^7.1.1",
    "jest": "^26.6.1",
    "prettier": "^1.19.1",
    "pretty-data": "^0.40.0",
    "rimraf": "^2.5.4",
    "webpack": "^5"
  },
  "dependencies": {
    "sitemap": "^6.0.0",
    "webpack-sources": "^1.4.3"
  },
  "resolutions": {
    "kind-of": "^6.0.3"
  }
}
